# Git 
alias gco="git checkout"
alias gcf="git clean -f -d"
alias gl="git pull"
alias gp="git push"
alias gpf="git push --force-with-lease"
alias gs="git status"
alias gr="git reset --hard"
alias gri="git rebase -i"

# Applications
alias lg="lazygit"

# Commands
alias srz="source ~/.zshrc"
alias sdt="stow --dotfiles"
